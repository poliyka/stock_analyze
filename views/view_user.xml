<?xml version="1.0"?>
<odoo>

  <!-- Stock -->
  <record id="form_stock_user" model="ir.ui.view">
    <field name="name">投資人</field>
    <field name="model">stock.user</field>
    <field name="arch" type="xml">
      <form string="投資人">
        <sheet>
          <group>
            <field name="name" />
          </group>
          <notebook>
            <page string="入帳紀錄">
              <field name="invest_ids" />
            </page>
          </notebook>
        </sheet>
      </form>
    </field>
  </record>

  <record id="tree_stock_user" model="ir.ui.view">
    <field name="name">投資人</field>
    <field name="model">stock.user</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name" />
      </tree>
    </field>
  </record>

</odoo>
